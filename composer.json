{
    "name": "elijahcruz/tasker",
    "description": "Run SSH tasks through a simple command line interface.",
    "license": "MIT",
    "authors": [
        {
            "name": "Elijah Cruz",
            "email": "elijah@ecwebservices.net"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "predis/predis": "^1.1",
        "symfony/console": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0",
        "vlucas/phpdotenv": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "bin": [
    "bin/tasker"
    ],
    "autoload": {
        "psr-4": {
            "Tasker\\Console\\": "src/",
            "Tasker\\Classes\\": "src/classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tasker\\Console\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
