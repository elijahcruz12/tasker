{
    "name": "elijahcruz/tasker",
    "description": "Easily deploy your projects through the command line.",
    "keywords": ["deployment", "console", "cli"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Elijah Cruz",
            "email": "elijah@ecwebservices.net"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/database": "^8.40",
        "laminas/laminas-text": "^2.8",

        "spatie/ssh": "^1.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.3",
        "laravel-zero/framework": "^8.8",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/tasker"]
}
