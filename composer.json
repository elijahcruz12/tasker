{
    "name": "elijahcruz/tasker",
    "description": "Easily deploy your projects through the command line.",
    "keywords": ["deployment", "console", "cli"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Elijah Cruz",
            "email": "elijah@ecwebservices.net"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-sqlite3": "*",
        "illuminate/database": "^9.12",
        "jc21/clitable": "^1.2",
        "laminas/laminas-text": "^2.8",
        "laravel-zero/phar-updater": "^1.3.0",
        "spatie/ssh": "^1.7",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.19",
        "mockery/mockery": "^1.4",
        "laravel-zero/framework": "^9",
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/tasker"]
}
